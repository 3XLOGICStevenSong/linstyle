package com.djb.ylt.framework.exception;

import java.lang.Exception;

/**
 * 
 * <H3>
 * アプリケーション例外クラス.</H3>
 * 
 * <PRE>
 * 復帰可能な例外クラス
 * または、アプリケーション固有の例外(SystemExceptionにセットする時に使用)
 * </PRE>
 * 
 * @author Wangjiquan
 */
public class ApplicationException extends Exception {

    /**
     * 
     */
    private static final long serialVersionUID = -3656655392198495858L;

    /**
     * <PRE>
     * 概要：コンストラクタ.
     * 詳細：
     * </PRE>
     * 
     * @param message
     *            詳細メッセージ
     * @param cause
     *            　 原因
     */
    public ApplicationException(String message, Exception cause) {
        super(message);
    }

    /**
     * <PRE>
     * 概要：コンストラクタ.
     * 詳細：
     * </PRE>
     * 
     * @param message
     *            詳細メッセージ
     */
    public ApplicationException(String message) {
        super(message);
    }

    /**
     * <PRE>
     * 概要：コンストラクタ.
     * 詳細：
     * </PRE>
     */
    public ApplicationException() {
        super();
    }
}
