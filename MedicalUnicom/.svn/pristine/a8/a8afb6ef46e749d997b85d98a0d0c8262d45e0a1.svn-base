.docDetail{font-size:0.4rem;}
.docFrom strong {position:relative}
.docFrom strong font{float:left;margin:0 0 0 -0.5rem;;width:0.5rem;height:1rem;line-height:1.2rem;text-align:center;font-size:0.6rem;}
.docDetail .doc_join{padding:0.2px 0 0;background:#fff;border-top:1px solid #d3d3d3;border-bottom:1px solid #d3d3d3}
.docDetail .doc_join .docFrom{width:9rem;min-height:1rem;margin:0 auto;border-bottom:1px solid #d3d3d3;line-height:1rem}
.docDetail .doc_join .docFrom.phone{border-bottom:1px solid #d3d3d3}
.docDetail .doc_join .docFrom strong{float:left;width:3rem;color:#4a4a4a;font-weight:normal}
.docDetail .doc_join .docFrom .date {padding:0 0 0 3rem}
.docDetail .doc_join .docFrom .date label{margin:0 0.3rem 0 0;}
.docDetail .doc_join .docFrom .date input{width:100%;height:0.7rem;border:0;font-size:0.4rem;}
.docDetail .doc_join .docFrom:last-child{border:0}
.docDetail .doc_join .docFrom.setMenu{border:0;}
.docDetail .doc_join .docFrom.setMenu input{width:1rem;text-align:center}
.docDetail .doc_join .docFrom select{height:0.7rem;font-size:0.35rem}

.docDetail .doc_join .docFrom.fileForm{padding:10px 0;text-align:center}
.docDetail .doc_join .docFrom.fileForm label{display:block;}

.docDetail .register {height:1.5rem;margin:0.5rem 0;background:#fff;;text-align:center;line-height:1.5rem;font-size:0.5rem;}
.docDetail .register span{display:block;color:#cc5d5e}

#sex label {margin:0 0.3rem 0 0;}
#sex label img{width:0.5rem;vertical-align:middle}

.bank_info {padding:0.3rem 0 0.3rem 0.2rem;text-align:left;color:#4a4a4a;font-size:0.4rem;}
.bank_info font{display:inline-block;font-size:0.6rem;}
.bank_date{padding:0.3rem 0;background:#fff;border-top:1px solid #d3d3d3;border-bottom:1px solid #d3d3d3}
.bank_date .docFrom{width:9rem;margin:0 auto;min-height:1rem;line-height:1rem;border-bottom:1px solid #d3d3d3}
.bank_date .docFrom.phone{border-bottom:1px solid #d3d3d3}
.bank_date .docFrom strong{float:left;width:3rem;color:#4a4a4a;font-weight:normal}
.bank_date .docFrom .date {padding:0 0 0 3rem}
.bank_date .docFrom .date input{width:100%;height:0.7rem;border:0;font-size:0.4rem;}
.bank_date .docFrom:last-child{border:0}

.SelFm.block{display:block;position:absolute;top:0;left:0;width:100%;height:100%;background:#fff;}


