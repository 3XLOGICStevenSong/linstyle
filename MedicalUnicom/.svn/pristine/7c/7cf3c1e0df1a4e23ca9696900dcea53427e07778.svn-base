.Prdoc .info {margin:10px 0 0 0;background:#fff;}
.Prdoc .info strong.UserImg{display:inline-block;padding:5% 3%;width:20%;vertical-align:top}
.Prdoc .info strong.UserImg img{width:100%;height:70px;border-radius:50%;}
.Prdoc .info  ul{display:inline-block;max-width:50%;padding:5% 0;vertical-align:top}
.Prdoc .info  ul li {padding:0 0 10px;}
.Prdoc .info  ul li.frt span{display:inline-block;padding:2px 10px;border-radius:15px;background:#50d071;color:#fff;}
.Prdoc .info  ul li.frt strong{font-weight:bold;}
.Prdoc .info  ul li strong{font-weight:normal;}
.Prdoc .info  ul li.frt strong span{font-weight:bold}
.Prdoc .info  ul li strong span{font-weight:bold}
.Prdoc .info  ul li .Symptom span{font-weight:normal;}

.Prdoc .info  ul li .docTitle,
.Prdoc .info  ul li .docProfile{display:block;}

.Prdoc .info .Price {float:right;padding:5% 3%;border-left:1px solid #d0d0d0}
.Prdoc .info .Price p{line-height:16px}
.Prdoc .info .Price p:first-child{margin:0 0 5px;color:#ff5f10}
.Prdoc .info .Price p .cam_icon{float:left;margin:0 5px 0 0;width:20%;height:15px;background:url("../img/cam_icon.png") no-repeat center center;background-size:100%;}
.Prdoc .info .Price p .call_icon{float:left;margin:0 5px 0 0;width:20%;height:15px;background:url("../img/call_icon.png") no-repeat center center;background-size:100%;}
.Prdoc .info .Price p.videoCam,
.Prdoc .info .Price p.calling{color:#919191}
