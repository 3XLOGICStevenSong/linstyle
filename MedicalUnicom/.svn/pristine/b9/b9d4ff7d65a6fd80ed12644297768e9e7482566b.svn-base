.moreList ul > li{margin:0 0 0.5rem}
.moreList .info {position:relative;background:#fff}
.moreList .info strong.UserImg{display:inline-block;padding:0.2rem;vertical-align:top}
.moreList .info strong.UserImg img{width:2rem;height:2rem;border-radius:100%;}
.moreList .info ul{display:inline-block;padding:0.2rem 0 0;vertical-align:top}
.moreList .info ul li {padding:0 0 0.3rem;margin:0;font-size:0.4rem;}
.moreList .info ul li.frt span{display:inline-block;padding:0.05rem 0.3rem;border-radius:15px;background:#50d071;color:#fff;font-size:0.35rem}
.moreList .info ul li.frt strong{font-weight:bold;}
.moreList .info ul li strong{font-weight:normal;}
.moreList .info ul li.frt strong span{font-weight:bold}
.moreList .info ul li strong span{font-weight:noemal}
.moreList .info ul li strong.Sex{margin:0 0.5rem 0 0}
.moreList .info span.gt_icon {position:absolute;right:3%;top:20px;color:#d4d4d4;font-size:24px;font-weight:bold}

.moreList ul li.loading{position:absolute;top:5rem;left:2.5rem;width:5rem;height:3rem;margin:1rem auto;text-align:center;line-height:3rem;background:#000;border-radius:0.2rem;opacity:0.3;}
.moreList ul li.loading img{vertical-align:middle}

.moreList .TimeList{border-top:1px solid #d0d0d0;background:#fff}

.moreList .state {padding:0.2rem 0.3rem 0.2rem;}
.moreList .state .timer{float:left;min-height:0.9rem;line-height:0.9rem;color:#7f7f7f;font-weight:normal}
.moreList .state .Notpaid,
.moreList .state .PaidOver,
.moreList .state .Refund,
.moreList .state .Wait,
.moreList .state .Success,
.moreList .state .Start,
.moreList .state .Count,
.moreList .state .Tel,
.moreList .state .Cam,
.moreList .state .Over,
.moreList .state .NotResult,
.moreList .state .DocReject,
.moreList .state .WaitCancel,
.moreList .state .AgreeCancel,
.moreList .state .Disagree,
.moreList .state .Cancel{float:right;color:#999;min-height:0.9rem;line-height:0.9rem;}

.moreList .state .Start span{float:left;width:2.3rem;min-height:0.9rem;line-height:0.9rem;background:#5faafc;text-align:center}
.moreList .state .Start span.sorry{float:left;min-height:0.9rem;line-height:0.9rem;background:#bfc1c2;}
.moreList .state .Start span a{display:block;padding:0 15px;color:#fff}

.moreList .state .Tel span{float:left;width:2.3rem;min-height:0.9rem;line-height:0.9rem;background:#5faafc;text-align:center}
.moreList .state .Tel span.callSorry{float:left;min-height:0.9rem;line-height:0.9rem;background:#bfc1c2;}
.moreList .state .Tel span a{display:block;padding:0 15px;color:#fff}

.moreList .state .Cancel span{float:left;width:2.3rem;min-height:0.9rem;line-height:0.9rem;background:#5faafc;text-align:center}
.moreList .state .Cancel span.callSorry{float:left;min-height:0.9rem;line-height:0.9rem;background:#bfc1c2;}
.moreList .state .Cancel span a{display:block;padding:0 15px;color:#fff}

.moreList .state .Count span{float:left;line-height:0.9rem;color:#818181}
.moreList .state .Count span.state_icon {float:left;width:0.5rem;height:0.9rem;;margin:0 0.02rem 0 0;background:url("../img/timer_icon.png") no-repeat 0 center;background-size:0.5rem;}

#MoreBtn{width:100%;height:1.5rem;padding:0.3rem 0;background: #fff;border-top:1px solid #999;border-bottom:1px solid #999;text-align:center;font-weight:bold;font-size:0.4rem;color:#f00}
