	<!-- 获取所有医生 -->
	<select id="findAllDoctor" parameterType="com.djb.ylt.user.entity.DoctorEntity"
		resultMap="DoctorResultMap">
		select
		<include refid="Doctor_Column_List" />
		,(SELECT MIN(ip.TOTAL) FROM interrogation_package ip WHERE
		d.DOCTOR_ID=ip.DOCTOR_ID )AS MIN_TOTAL,
		(
		SELECT
		AVG(dc.GRADE)
		FROM
		doctor_comment dc
		WHERE
		dc.DOCTOR_ID =d.DOCTOR_ID
		)AS COMMENT_GRADE
		<if test="patientId != null">,f.F_ID</if>
		from doctor d
		<if test="patientId != null">LEFT OUTER JOIN follow f ON (d.DOCTOR_ID=f.DOCTOR_ID and
			f.PATIENT_ID=#{patientId})
		</if>
	</select>
	<select id="getObjectForInquery" parameterType="com.djb.ylt.user.entity.DoctorEntity"
		resultMap="DoctorResultMap">
		select
		<include refid="Doctor_Column_List" />
		,ip.TOTAL,
		<include refid="InterrogationPackage_Column_List" />
		,
		<!-- MAX(ip.TOTAL) AS MAX_TOTAL,MIN(ip.TOTAL) AS MIN_TOTAL -->
		(
		SELECT
		AVG(dc.GRADE)
		FROM
		doctor_comment dc
		WHERE
		dc.DOCTOR_ID =
		#{doctorId}
		)AS COMMENT_GRADE
		<if test="patientId!=null">,(SELECT COUNT(*) FROM follow f WHERE
			f.DOCTOR_ID=#{doctorId} and f.PATIENT_ID=#{patientId}) AS
			FOLLOW_COUNT
		</if>
		from doctor d LEFT JOIN interrogation_package ip
		ON(d.DOCTOR_ID=ip.DOCTOR_ID and ip.TYPE='0')
		where 1=1 AND
		d.STATUS_FLG='1'
		<if test="doctorId!=null">and d.DOCTOR_ID=#{doctorId} </if>
		<if test="userId!=null">and d.USER_ID=#{userId} </if>
	</select>
	<!-- Get geneDoctor -->
	<select id="findListForGene" parameterType="com.djb.ylt.user.entity.DoctorEntity"
		resultMap="DoctorResultMap">
		select
		<include refid="Doctor_Column_List" />
		,
		(select AVG(dc.GRADE) from doctor_comment dc where
		dc.DOCTOR_ID=d.DOCTOR_ID) AS COMMENT_GRADE
		<if test="patientId != null">,f.F_ID</if>
		from doctor d
		<if test="patientId != null">LEFT OUTER JOIN follow f ON (d.DOCTOR_ID=f.DOCTOR_ID and
			f.PATIENT_ID=#{patientId})
		</if>
		where 1=1
		<if test="doctorType !=null">and d.DOCTOR_TYPE=#{doctorType} </if>
		ORDER BY COMMENT_GRADE DESC, d.DOCTOR_ID
		limit #{pageNum},#{pageSize}
	</select>
