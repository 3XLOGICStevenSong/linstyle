.NavGuige {padding:0.5rem 0 0;background:#fff;}
.NavGuige ul {border-bottom:1px solid #999;text-align:center;font-size:0;line-height:0}
.NavGuige ul li{position:relative;display:inline-block;width:1.6rem;height:1rem;line-height:1rem;border-top:1px solid #999;font-size:0.3rem;text-align:center;}
.NavGuige ul li span{position:absolute;top:-0.15rem;left:0.65rem;width:0.3rem;height:0.3rem;background:#999;border-radius:50%}
.NavGuige ul li.on {border-top:1px solid #52abfb;}
.NavGuige ul li.on span{background:#52abfb;}
.NavGuige ul li:first-child{width:2rem;}

.NavGuige.buyed ul li{width:2.4rem}
.NavGuige.buyed ul li span{position:absolute;left:1.05rem}

.opener{display:inline-block;position:relative;width:100px;height:50px;background:#999;border-radius:25px;}
.opener span{display:inline-block;position:absolute;top:2px;left:2px;width:46px;height:46px;background:#fff;border-radius:100%;}
.opener.On {background:#4ad764;}
.opener.On span{background:#fff;position:absolute;top:2px;left:52px;}

.Mess{position:fixed;top:30%;left:2rem;width:6rem;height:3rem;z-index:999}
.Mess .MessWrap{position:relative;}
.Mess .bg{display:block;width:100%;height:3rem;background:#000;border-radius:10px;opacity:0.5;}
.Mess .imgComplate{position:absolute;top:0.7rem;width:100%;text-align:center}
.Mess .imgComplate img{width:1rem;}
.Mess font{position:absolute;bottom:0.5rem;width:100%;text-align:center;}

.Mess1{position:fixed;top:30%;left:1rem;width:8rem;height:3rem;border:1px solid #ccc;background:#fff;border-radius:20px;z-index:11}
.Mess1 .MessWrap{position:relative;width:100%;height:3rem;}
.Mess1 .tip{position:absolute;top:0.7rem;width:100%;text-align:center;font-size:0.5rem;color:#000;font-weight:bold}
.Mess1 .cancel{position:absolute;bottom:0;left:0;width:50%;height:1.2rem;line-height:1.2rem;border-top:1px solid #ccc;border-right:1px solid #ccc;text-align:center;color:#52abfb;font-size:0.5rem;}
.Mess1 .Gologin{position:absolute;bottom:0;right:0;width:50%;height:1.2rem;line-height:1.2rem;border-top:1px solid #ccc;text-align:center;color:#52abfb;font-size:0.5rem;}

.Mess2{position:fixed;bottom:-6rem;width:100%;z-index:11}
.Mess2 .MessWrap{position:relative;width:9rem;height:5.5rem;margin:0 auto}
.Mess2 .confrim{position:relative;width:9rem;height:3rem;background:#fff;border-radius:20px;text-align:center}
.Mess2 .confrim p{height:1.5rem;line-height:1.5rem;text-align:center;font-size:0.5rem;border-bottom:1px solid #ccc;}
.Mess2 .confrim span{display:block;height:1.5rem;line-height:1.5rem;font-size:0.5rem;}
.Mess2 .cancel{display:block;height:1.5rem;margin:0.5rem 0 0;line-height:1.5rem;background:#fff;border-radius:20px;text-align:center;color:#52abfb;font-size:0.5rem;}

.blind{position:fixed;top:0;width:100%;height:100%;background:#000;opacity:0.3;z-index:10}