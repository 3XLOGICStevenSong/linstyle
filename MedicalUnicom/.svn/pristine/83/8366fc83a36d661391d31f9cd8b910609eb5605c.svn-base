@charset "utf-8";

*{margin:0;padding:0}
html,body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,img { margin:0; padding:0; }
html { width:100%;height:100%;font-family: Helvetica }
body {position:relative;height:100%;}
body p{font-size:0.35rem;}
body,code {color:#353535;font-size:0.35rem; background:#ebebeb; }
#main {position:relative;}
li { list-style:none; }
img,fieldset { border:none; vertical-align:top; }
table { width:100%; border:0; border-spacing:0; border-collapse:collapse; }
caption { display:none; }
th,td { border:0; vertical-align:top; }
input{background:#fff}
input,select,textarea {color:#353535; vertical-align:middle; }
.inputTypeText { height:19px; padding:3px 0 0 9px; border:1px solid #d5d5d5; }
select { height:24px; border:1px solid #d5d5d5; }
textarea { padding:0.2rem; border:1px solid #d5d5d5; line-height:0.5rem; }
legend { visibility:hidden; position:absolute; left:-9999px; top:-9999px; width:0; height:0; line-height:0; }
button { overflow:visible; padding:0; margin:0; border:0; cursor:pointer; }
hr.layout { display:none; }
a { text-decoration:none; color:#000; }
a:hover { text-decoration:none; }
a:active { text-decoration:none; }
.objHidden { visibility:hidden; position:absolute; left:-1000px; top:-1000px; height:0; width:0; }
table tr.radioType input,
span.noBorder input { border:none !important; width:auto !important; height:auto !important; margin:0 3px 0 0 !important; vertical-align:middle !important; background:none !important; }
#content_CONTAINER td { width:auto !important; }
table.nneditor-table { width:auto !important; }

.displaynone{display:none !important;}
.clearfix:after{content:"";display:block;clear:both}
.f_left{float:left}
.f_right{float:right}

.loading{position:absolute;top:5rem;left:2.5rem;width:5rem;height:3rem;margin:1rem auto;text-align:center;line-height:3rem;background:#000;border-radius:0.2rem;opacity:0.3;}
.loading img{vertical-align:middle}

.titleArea{display:none !important;}
