.contentList .Menu{display:none;}
.contentList .Menu.history{display:block}
.history{ padding:0 0.2rem;background:#fff;font-size:0.4rem}
.history ul{width:8.5rem;margin:0 auto;padding:0.5rem 0}
.history ul li {position:relative;height:2rem;padding:0 0 0 1rem;;line-height:1rem;border-left:1px solid #d8d8d8;}
.history ul li:last-child{border:0}
.history ul li:first-child p{color:#228ce1}
.history ul li:first-child p span{color:#353535}
.history ul li span.icon{position:absolute;left:-0.5rem}
.history ul li span.icon img{width:1rem;height:1rem}
.history ul li p{padding:0 0 0.3rem;height:1rem;line-height:1rem;border-bottom:1px solid #d8d8d8}
.history ul li p span{float:right}

.historyInfo .Dep_Date {padding:0.5rem 0.2rem;background:#fff;}
.historyInfo .Dep_Date p{height:1rem;line-height:1rem}
.historyInfo .Dep_Date .Dep{float:left;}
.historyInfo .Dep_Date .Dep strong{font-size:0.4rem}
.historyInfo .Dep_Date .Date{float:right;}
.historyInfo .Dep_Date .Date img{margin:0 0.1rem 0 0;vertical-align:middle}
.historyInfo .Dep_Date_info {position:relative;margin:1.2rem 0 0 0;padding:1.2rem 0.2rem;background:#fff;}
.historyInfo .Dep_Date_info .tImg{position:absolute;top:-0.7rem}
.historyInfo .Dep_Date_info .tImg img{width:1.5rem;}
.historyInfo .Dep_Date_info .tTxt{padding:0 0.3rem;font-size:0.4rem;line-height:0.5rem}

.ImageFile {margin:1.3rem 0 0 0;padding:0 0.2rem;background:#fff;border-bottom:1px solid #d8d8d8}
.ImageFile .File {position:relative;height:1rem;line-height:1rem}
.ImageFile .File .tImg{position:absolute;top:-0.7rem}
.ImageFile .File .tImg img{width:1.5rem}
.ImageFile .File .Date{float:right;}
.ImageFile .File .Date img{margin:0 0.1rem 0 0;vertical-align:middle}
.ImageFile .FileList .imgList{padding:0.5rem 0;overflow:auto;white-space:nowrap;}
.ImageFile .FileList .imgList img{width:3rem;height:3rem;margin:0 0.2rem 0 0}
.ImageFile .FileList .imgList p.Empty{height:1.5rem;line-height:1.5rem;text-align:center;}

.Analsis {padding:0 0.2rem;background:#fff}
.Analsis p.Date{padding:0.5rem 0;height:1rem;line-height:1rem;border-bottom:1px solid #cdcdcd}
.Analsis p img{margin:0 0.1rem 0 0;vertical-align:middle}
.Analsis .AsisGuid p{padding:0.5rem 0.3rem;line-height:0.5rem;color::#000;font-size:0.4rem}
.Analsis .AsisGuid p span{color:#999;font-size:0.4rem}

.info {padding:0.2rem 0;background:#fff;border-bottom:1px solid #cdcdcd;font-size:0.4rem}
.info .UserImg{float:left;padding:0.2rem;}
.info .UserImg img{width:2rem;height:2rem;max-width:2rem;max-height:2rem;border-radius:100%;}
.info ul {float:left;width:5rem;margin:0.2rem;border-right:1px solid #999}
.info ul li{padding:0 0 0.2rem}
.info ul li strong.UsrData {float:left;padding:0.5rem 0 0;font-weight:normal}
.info ul li strong.UsrData span{float:left;height:0.6rem;padding:0 0.2rem;margin:0 0.15rem 0 0;line-height:0.6rem;background:#cdcdcd;border-radius: 20px;color: #fff;font-size:0.35rem;}
.info ul li strong.UsrData span.UsrSex{background:#50d071}
.info ul li strong.UsrData span img{margin:0 0.1rem 0 0;vertical-align:middle}

.info .UsrPrivacy{float:right;width:1.75rem;padding:0.5rem 0.2rem;text-align:center}
.info .UsrPrivacy p{margin:0.5rem 0 0;color:#75adea}

.NaviCate {margin:0.3rem 0 0 0;background:#fff;border-top:1px solid #cdcdcd;}
.NaviCate ul {line-height:0;font-size:0;border-bottom:1px solid #999;}
.NaviCate ul li{display:inline-block;width:2.5rem;padding:0.2rem 0;border-bottom:2px solid transparent;font-size:0.4rem}
.NaviCate ul li span{display:block;height:0.7rem;line-height:0.7rem;text-align:center;border-left:1px solid #999;}
.NaviCate ul li:first-child span{border:0}
.NaviCate ul li.On {border-bottom:0.1rem solid #5babfc;font-weight:bold}

#MoreBtn{width:100%;height:1.5rem;padding:0.3rem 0;background:#fff;border-top:1px solid #999;border-bottom:1px solid #999;text-align:center;font-weight:bold;font-size:0.4rem;color:#f00}
