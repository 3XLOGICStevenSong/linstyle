<!DOCTYPE HTML>
<html lang="en-US">
<head>
	<meta charset="UTF-8">
	<script type="text/javascript" src="js/Device_rem.js"></script>
	<title></title>
	<link rel="stylesheet" href="css/common.css" />
	<link rel="stylesheet" href="css/gradList.css" />
	<script type="text/javascript" src="js/jquery-1.11.2.min.js"></script>
	<script type="text/javascript" src="js/login.js"></script>
	<script type="text/javascript" src="js/ajaxDocGradelist.js"></script>
</head>
<body>

	<div id="header">
		<div class="titleArea">
			<span class="prev"><a href="javascript:history.go(-1)">&lt;</a></span>
			<h2>用户评价</h2>
		</div>
	</div>
	 
	<div id="wrap">

		<div id="container">
			<div class="contents">
				<div class="gradList">
					<ul class="List displaynone">
						<li class="li">
							<div class="info">
								<p class="Title clearfix">
									<strong class="UserName">{$name}</strong>
									<strong class="grad">评分: <span class="point">{$grade}</span></strong>
								</p>
								<p class="cdtInfo">{$content}</p>
								<p class="date clearfix"><span class="Times">{$createTime}</span></p>
							</div>
						</li>
					</ul>
					<ul Id="List">
						<li class="loading"><img src="img/loading.gif"></li>
					</ul>
				</div>
			</div>
		</div>
		
		<div id="footer"></div>
	</div>
	
</body>
</html>